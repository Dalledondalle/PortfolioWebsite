@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@MelanWeb.StaticInformation.ContactInformations.Name</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- <link rel="manifest" href="site.webmanifest"> -->
    <!-- Place favicon.ico in the root directory -->
    <!-- CSS here -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/magnific-popup.css">
    <link rel="stylesheet" href="css/font-awesome.min.css">
    <link rel="stylesheet" href="css/themify-icons.css">
    <link rel="stylesheet" href="css/nice-select.css">
    <link rel="stylesheet" href="css/flaticon.css">
    <link rel="stylesheet" href="css/gijgo.css">
    <link rel="stylesheet" href="css/animate.min.css">
    <link rel="stylesheet" href="css/slick.css">
    <link rel="stylesheet" href="css/slicknav.css">
    <link rel="stylesheet" href="css/style.css">
    <!-- <link rel="stylesheet" href="css/responsive.css"> -->
</head>

<body>
    <!-- header-start -->
    <header>
        <div class="header-area ">
            <div id="sticky-header" class="main-header-area">
                <div class="container-fluid">
                    <div class="row align-items-center">
                        <div class="col-xl-3 col-lg-2">
                            <div class="logo">
                                <a href="Index">
                                    <img src="img/logo.png" alt="">
                                </a>
                            </div>
                        </div>
                        <div class="col-xl-6 col-lg-7">
                            <div class="main-menu  d-none d-lg-block">
                                <nav>
                                    <ul id="navigation">
                                        <li><a class="active" href="Index">Hjem</a></li>
                                        <li><a href="AboutMe">Om mig</a></li>
                                        <li>
                                            <a href="#">Fagligt<i class="ti-angle-down"></i></a>
                                            <ul class="submenu">
                                                <li><a href="Portfolio">Portfolio</a></li>
                                                <li><a href="Experiences">Erfaringer</a></li>
                                            </ul>
                                        </li>
                                        <li><a href="Contact">Contact</a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                        <div class="col-xl-3 col-lg-3 d-none d-lg-block">
                            <div class="Appointment">
                                <div class="book_btn d-none d-lg-block">
                                    <a href="Contact">Kontakt Mig</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </header>
    <!-- header-end -->
    <!-- slider_area_start -->
    <div class="slider_area">
        <div class="single_slider  d-flex align-items-center slider_bg_1">
            <div class="shap_pattern d-none d-lg-block">
                <img src="img/about/grid.png" alt="">
            </div>
            <div class="social_links">
                <ul>
                    <li><a href=@MelanWeb.StaticInformation.ContactInformations.Facebook target="_blank"> <i class="fa fa-facebook"></i> </a></li>
                    <li><a href=@MelanWeb.StaticInformation.ContactInformations.Twitter target="_blank"> <i class="fa fa-twitter"></i> </a></li>
                    <li><a href=@MelanWeb.StaticInformation.ContactInformations.LinkedIn target="_blank"> <i class="fa fa-linkedin"></i> </a></li>
                </ul>
            </div>
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-12">
                        <div class="slider_text text-center">
                            <h3>
                                Velkommen, dette er Daniel <br />
                                Kristensen
                            </h3>
                            <span>Programmer</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- slider_area_end -->
    <!-- service_area  -->
    <div class="service_area">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="section_title text-center mb-65">
                        <span>Service Provided</span>
                        <h3>
                            Lave programmer  <br>
                            & hjemmesider
                        </h3>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-4 col-md-4">
                    <div class="single_service text-center">
                        <div class="icon">
                            <img src="img/svg_icon/1.svg" alt="">
                        </div>
                        <h3>Programmering</h3>
                        <p>Både i C#, Python, Java og C++</p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_service text-center">
                        <div class="icon">
                            <img src="img/svg_icon/2.svg" alt="">
                        </div>
                        <h3>Hjemmesider</h3>
                        <p>I ASP.NET eller ren HTML/CSS/Javascript</p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_service text-center">
                        <div class="icon">
                            <img src="img/svg_icon/3.svg" alt="">
                        </div>
                        <h3>Backend</h3>
                        <p>Backend funktionalitet til lige hvad du skal bruge</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ service_area  -->
    <!-- portfolio_area -->
    <div class="portfolio_area portfolio_bg_1">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="section_title white_text text-center">
                        <span>Portfolios</span>
                        <h3>
                            Nogle af mine <br>
                            projekter
                        </h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ portfolio_area -->
    <!-- portfolio_image_area  -->
    <!-- Githubs her-->
    <div class="portfolio_image_area">
        <div class="container">
            <div class="row">
                <div class="col-xl-5 col-md-5">
                    <div class="single_Portfolio">
                        <div class="portfolio_thumb">
                            <img src=@GithubPortfolio.Content.PortfolioObjects[0].IssueObjects[0].ImgURL alt="">
                        </div>
                        <a href=@GithubPortfolio.Content.PortfolioObjects[0].IssueObjects[0].ImgURL class="popup popup-image"></a>
                        <div class="portfolio_hover">
                            <div class="title">
                                <h3>Product Design</h3>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-7 col-md-7">
                    <div class="single_Portfolio">
                        <div class="portfolio_thumb">
                            <img src=@GithubPortfolio.Content.PortfolioObjects[1].IssueObjects[0].ImgURL alt="">
                        </div>
                        <a href=@GithubPortfolio.Content.PortfolioObjects[1].IssueObjects[0].ImgURL class="popup popup-image"></a>
                        <div class="portfolio_hover">
                            <div class="title">
                                <h3>Product Design</h3>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-4 col-md-6 col-lg-4">
                    <div class="single_Portfolio">
                        <div class="portfolio_thumb">
                            <img src=@GithubPortfolio.Content.PortfolioObjects[2].IssueObjects[0].ImgURL alt="">
                        </div>
                        <a href=@GithubPortfolio.Content.PortfolioObjects[2].IssueObjects[0].ImgURL class="popup popup-image"></a>
                        <div class="portfolio_hover">
                            <div class="title">
                                <h3>Product Design</h3>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-4 col-md-6 col-lg-4">
                    <div class="single_Portfolio">
                        <div class="portfolio_thumb">
                            <img src=@GithubPortfolio.Content.PortfolioObjects[3].IssueObjects[0].ImgURL alt="">
                        </div>
                        <a href=@GithubPortfolio.Content.PortfolioObjects[3].IssueObjects[0].ImgURL class="popup popup-image"></a>
                        <div class="portfolio_hover">
                            <div class="title">
                                <h3>Product Design</h3>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-xl-4 col-md-12 col-lg-4">
                    <div class="single_Portfolio">
                        <div class="portfolio_thumb">
                            <img src=@GithubPortfolio.Content.PortfolioObjects[4].IssueObjects[0].ImgURL alt="">
                        </div>
                        <a href=@GithubPortfolio.Content.PortfolioObjects[4].IssueObjects[0].ImgURL class="popup popup-image"></a>
                        <div class="portfolio_hover">
                            <div class="title">
                                <h3>Product Design</h3>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-12">
                    <div class="more_portfolio text-center">
                        <a class="line_btn" href="Portfolio">Mere Portfolio</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ portfolio_image_area  -->
    <!-- about_me  -->
    <div class="about_me">
        <div class="about_large_title d-none d-lg-block">
            About
        </div>
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-md-6">
                    <div class="about_e_details">
                        <h3>Om Mig</h3>
                        <p>Engageret programmør med flair for backend og optimering af dette. Mere om dette i <a href="AboutMe">om mig</a></p>
                        <div class="download_cv">
                            <a class="boxed-btn3" href="#">Download CV</a>
                        </div>
                    </div>
                </div>
                <div class="col-xl-6 col-md-6">
                    <div class="about_img">
                        <div class="color_pattern d-none d-lg-block">
                            <img src="img/about/color_grid.png" alt="">
                        </div>
                        <div class="my_Pic">
                            <img src="img/about/about.png" alt="">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--/ about_me  -->

    <div class="counter_area">
        <div class="container">
            <div class="row">
                <div class="col-xl-4 col-md-4">
                    <div class="single_counter text-center">
                        <h3>
                            <span class="counter">@GithubPortfolio.Projects </span><span>+</span>
                        </h3>
                        <p>Samlet Projekter</p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_counter text-center">
                        <h3>
                            <span class="counter">@GithubPortfolio.OnGoingProjects </span>
                        </h3>
                        <p>Igangværende</p>
                    </div>
                </div>
                <div class="col-xl-4 col-md-4">
                    <div class="single_counter text-center">
                        <h3>
                            <span class="counter">@GithubPortfolio.SuccesRate</span> <span>%</span>
                        </h3>
                        <p>Job Success</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
